{
  "name": "yet-another-package-json-dependencies-updates-checker-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "dependencies": {
    "@smui/button": "6.0.0-beta.16",
    "@smui/form-field": "6.0.0-beta.16",
    "@smui/layout-grid": "6.0.0-beta.16",
    "@smui/linear-progress": "6.0.0-beta.16",
    "@smui/radio": "6.0.0-beta.16",
    "@smui/ripple": "6.0.0-beta.16",
    "@smui/snackbar": "6.0.0-beta.16",
    "@smui/textfield": "6.0.0-beta.16",
    "@sveltestack/svelte-query": "1.6.0",
    "axios": "0.27.2",
    "svelte-file-dropzone": "1.0.0-dev.1",
    "svelte-i18n": "3.4.0",
    "svelte-material-ui": "6.0.0-beta.16"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.42",
    "@tsconfig/svelte": "3.0.0",
    "broadcast-channel": "4.11.0",
    "smui-theme": "6.0.0-beta.16",
    "svelte": "3.47.0",
    "svelte-check": "2.7.0",
    "svelte-preprocess": "4.10.6",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "vite": "2.9.6"
  }
}
