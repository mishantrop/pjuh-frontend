{
  "name": "yet-another-package-json-dependencies-updates-checker-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.0",
    "rollup": "2.70.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "7.0.0",
    "rollup-plugin-terser": "7.0.0",
    "smui-theme": "6.0.0-beta.15",
    "svelte": "3.46.6"
  },
  "dependencies": {
    "@smui/button": "6.0.0-beta.15",
    "@smui/textfield": "6.0.0-beta.15",
    "@sveltestack/svelte-query": "1.6.0",
    "axios": "0.26.1",
    "sirv-cli": "2.0.0",
    "svelte-material-ui": "6.0.0-beta.15"
  }
}
